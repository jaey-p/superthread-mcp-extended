{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "superthread",
	"main": "src/index.ts",
	"compatibility_date": "2025-03-10",
	"compatibility_flags": ["nodejs_compat"],
	"observability": {
		"enabled": true,
		"head_sampling_rate": 1
	},
	"assets": {
		"directory": "./static/",
		"binding": "ASSETS"
	},
	"env": {
		"staging": {
			"name": "superthread-staging",
			"observability": {
				"enabled": true,
				"head_sampling_rate": 1
			}
		},
		"production": {
			"name": "superthread-production",
			"observability": {
				"enabled": true,
				"head_sampling_rate": 0.1
			}
		}
	}
}
